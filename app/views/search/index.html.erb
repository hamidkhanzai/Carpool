<div style="height: 400px">
<%= form_tag({:action=>'list'},:method => "post", :class => "") do %>




    <div>
      <table  >
        <tr>
          <td><%= label_tag "from"%></td>
          <td><%= select_tag :d_from, options_for_select(["Kabul", "Herat","Kandahar","Nangarhar","Balkh", "Sarepool"])%> </td>
        </tr>
        <tr>
          <td> <%= label_tag "Distanation"%></td>
          <td><%= select_tag :d_to, options_for_select(["Kabul", "Herat","Kandahar","Nangarhar","Balkh", "Sarepool"]) %></td>
        </tr>
        <tr>
          <td><%= label_tag "Time"%></td>
          <td><%= text_field_tag 'travel_t', nil, :placeholder => 'example 12:00' %> </td>
        </tr>
        <tr class="controls">
          <td><%= label_tag "Date" %></td>
     <td>   <input type="text" name="travel_date"  data-behaviour='datepicker' ></td>


          </tr>
        <tr>
          <td colspan="2"><%= submit_tag "Search",:name=>nil,:class=>"btn btn-primary" %></td>
        </tr>
          </table>
        </div>
 <% end %>
<script type="text/javascript">
    $('[data-behaviour~=datepicker]').datepicker({
     dateFormat: 'yy-mm-dd'
    })
</script>


</div>